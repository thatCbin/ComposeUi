<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cbin.composeui">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CompseUi">
        <activity
            android:name="com.cbin.composeui.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.CompseUi.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.cbin.composeui.layout.RowActivity" />
        <activity android:name="com.cbin.composeui.HomeActivity" />
        <activity android:name="com.cbin.composeui.layout.LayoutActivity" />
        <activity android:name="com.cbin.composeui.ThemeActivity" />

    </application>

</manifest>